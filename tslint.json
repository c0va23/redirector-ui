{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-standard"
    ],
    "jsRules": {},
    "rules": {
      "max-line-length": [true, 100],
      "max-file-line-count": [true, 300],
      "trailing-comma": [
        true, {
          "multiline": "always",
          "singleline": "never"
        }
      ]
    },
    "rulesDirectory": []
}
